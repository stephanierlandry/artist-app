{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
      "no-param-reassign": [2, { "props": false }],
      "no-shadow": [2, {"builtinGlobals": false, "hoist": "never", "allow": ["pokemon"]}],
      "no-console": "off"
    }
}
